{
  "schema": {
    "types": [
      {
        "name": "Root",
        "actions": [
          {
            "name": "configure",
            "type": "Void",
            "params": [
              {
                "name": "token",
                "type": "String",
                "description": "Authentication token for accessing Pinecone services"
              },
              {
                "name": "environment",
                "type": "String",
                "description": "Specifies the environment to target within Pinecone services"
              }
            ],
            "description": "Initializes the driver with the necessary configuration for Pinecone operations"
          }
        ],
        "fields": [
          {
            "name": "indexes",
            "type": "IndexCollection",
            "description": "Collection of Pinecone indexes available for operations"
          },
          {
            "name": "status",
            "type": "String",
            "description": "Current status of the Pinecone driver"
          },
          {
            "name": "tests",
            "type": "Tests",
            "description": "Collection of tests available for the Pinecone driver"
          }
        ],
        "description": "Interface for interacting with Pinecone services"
      },
      {
        "name": "IndexCollection",
        "fields": [
          {
            "name": "one",
            "type": "Index",
            "params": [
              {
                "name": "name",
                "type": "String"
              }
            ]
          },
          {
            "name": "items",
            "type": "List",
            "ofType": "Index"
          }
        ],
        "actions": [
          {
            "name": "create",
            "type": "Void",
            "params": [
              {
                "name": "name",
                "type": "String"
              },
              {
                "name": "dimension",
                "type": "Int"
              },
              {
                "name": "metric",
                "type": "String"
              },
              {
                "name": "replicas",
                "type": "Int"
              },
              {
                "name": "pod_type",
                "type": "String"
              },
              {
                "name": "metadata_config",
                "type": "String"
              },
              {
                "name": "source_collection",
                "type": "String"
              },
              {
                "name": "pods",
                "type": "Int"
              }
            ]
          }
        ]
      },
      {
        "name": "Index",
        "fields": [
          {
            "name": "database",
            "type": "Database"
          },
          {
            "name": "status",
            "type": "Status"
          }
        ],
        "actions": [
          {
            "name": "query",
            "type": "String",
            "params": [
              {
                "name": "namespace",
                "type": "String",
                "optional": true
              },
              {
                "name": "id",
                "type": "String",
                "optional": true
              },
              {
                "name": "top_k",
                "type": "Int",
                "optional": true
              },
              {
                "name": "includeValues",
                "type": "Boolean",
                "optional": true
              },
              {
                "name": "includeMetadata",
                "type": "Boolean",
                "optional": true
              },
              {
                "name": "vector",
                "type": "String",
                "optional": true
              },
              {
                "name": "filter",
                "type": "String",
                "optional": true
              },
              {
                "name": "sparceVector",
                "type": "String",
                "optional": true
              }
            ]
          },
          {
            "name": "upsert",
            "type": "Void",
            "params": [
              {
                "name": "namespace",
                "type": "String",
                "optional": true
              },
              {
                "name": "vectors",
                "type": "String",
                "optional": true
              }
            ]
          }
        ]
      },
      {
        "name": "Database",
        "fields": [
          {
            "name": "name",
            "type": "String",
            "description": "The name of the database."
          },
          {
            "name": "metric",
            "type": "String",
            "description": "The metric type used by the database."
          },
          {
            "name": "dimension",
            "type": "Int",
            "description": "The number of dimensions for the database's indexing vector."
          },
          {
            "name": "replicas",
            "type": "Int",
            "description": "The number of replicas for each shard in the database."
          },
          {
            "name": "shards",
            "type": "Int",
            "description": "The total number of shards across all replicas in the database."
          },
          {
            "name": "pods",
            "type": "Int",
            "description": "The total number of pods allocated to the database."
          },
          {
            "name": "pod_type",
            "type": "String",
            "description": "The type of pods used by the database."
          }
        ],
        "description": "Represents a database configuration within the Pinecone service."
      },
      {
        "name": "Status",
        "fields": [
          {
            "name": "waiting",
            "type": "List",
            "ofType": "String"
          },
          {
            "name": "crashed",
            "type": "List",
            "ofType": "String"
          },
          {
            "name": "host",
            "type": "String"
          },
          {
            "name": "port",
            "type": "Int"
          },
          {
            "name": "state",
            "type": "String"
          },
          {
            "name": "ready",
            "type": "Boolean"
          }
        ]
      },
      {
        "name": "Tests",
        "actions": [
          {
            "name": "testGetDatabases",
            "type": "Boolean",
            "description": "Initiates a test to retrieve a list of databases."
          }
        ],
        "description": "Handles the testing operations for the membrane-driver-pinecone program."
      }
    ]
  },
  "dependencies": {
    "http": "http:"
  }
}