{
  "schema": {
    "types": [
      {
        "name": "Root",
        "actions": [
          {
            "name": "configure",
            "type": "Void",
            "params": [
              {
                "name": "token",
                "type": "String",
                "description": "API access token for authenticating with the Pinecone service."
              },
              {
                "name": "environment",
                "type": "String",
                "description": "The Pinecone environment to connect to ('default' if not specified)."
              }
            ],
            "description": "Configures the Pinecone connection with a provided API token and optional environment setting."
          }
        ],
        "fields": [
          {
            "name": "indexes",
            "type": "IndexCollection",
            "description": "Collection of Pinecone index configurations."
          },
          {
            "name": "status",
            "type": "String",
            "description": "The current status of the Pinecone service connection."
          },
          {
            "name": "tests",
            "type": "Tests",
            "description": "A set of test utilities for the Pinecone indexes."
          }
        ],
        "description": "Manages interactions with the Pinecone database service."
      },
      {
        "name": "IndexCollection",
        "fields": [
          {
            "name": "one",
            "type": "Index",
            "params": [
              {
                "name": "name",
                "type": "String",
                "description": "The unique name of the pinecone index"
              }
            ],
            "description": "Retrieves a single pinecone index by its name"
          },
          {
            "name": "items",
            "type": "List",
            "ofType": "Index",
            "description": "A list of all pinecone indexes"
          }
        ],
        "actions": [
          {
            "name": "create",
            "type": "Void",
            "params": [
              {
                "name": "name",
                "type": "String",
                "description": "Name for the new pinecone index"
              },
              {
                "name": "dimension",
                "type": "Int",
                "description": "The number of dimensions of the pinecone index"
              },
              {
                "name": "metric",
                "type": "String",
                "description": "The metric type to measure distance in the pinecone index"
              },
              {
                "name": "replicas",
                "type": "Int",
                "description": "The number of replicas for each pod in the pinecone index"
              },
              {
                "name": "pod_type",
                "type": "String",
                "description": "The type of pod to use for the pinecone index"
              },
              {
                "name": "metadata_config",
                "type": "String",
                "description": "Configuration for metadata in the pinecone index"
              },
              {
                "name": "source_collection",
                "type": "String",
                "description": "Optional source collection to import data from when creating the pinecone index"
              },
              {
                "name": "pods",
                "type": "Int",
                "description": "Initial number of pods to allocate for the pinecone index"
              }
            ],
            "description": "Creates a new pinecone index with customizable parameters"
          }
        ],
        "description": "Collection of pinecone indexes"
      },
      {
        "name": "Index",
        "fields": [
          {
            "name": "database",
            "type": "Database"
          },
          {
            "name": "status",
            "type": "Status"
          }
        ],
        "actions": [
          {
            "name": "query",
            "type": "String",
            "params": [
              {
                "name": "namespace",
                "type": "String",
                "optional": true
              },
              {
                "name": "id",
                "type": "String",
                "optional": true
              },
              {
                "name": "top_k",
                "type": "Int",
                "optional": true
              },
              {
                "name": "includeValues",
                "type": "Boolean",
                "optional": true
              },
              {
                "name": "includeMetadata",
                "type": "Boolean",
                "optional": true
              },
              {
                "name": "vector",
                "type": "String",
                "optional": true
              },
              {
                "name": "filter",
                "type": "String",
                "optional": true
              },
              {
                "name": "sparceVector",
                "type": "String",
                "optional": true
              }
            ]
          },
          {
            "name": "upsert",
            "type": "Void",
            "params": [
              {
                "name": "namespace",
                "type": "String",
                "optional": true
              },
              {
                "name": "vectors",
                "type": "String",
                "optional": true
              }
            ]
          }
        ]
      },
      {
        "name": "Database",
        "fields": [
          {
            "name": "name",
            "type": "String"
          },
          {
            "name": "metric",
            "type": "String"
          },
          {
            "name": "dimension",
            "type": "Int"
          },
          {
            "name": "replicas",
            "type": "Int"
          },
          {
            "name": "shards",
            "type": "Int"
          },
          {
            "name": "pods",
            "type": "Int"
          },
          {
            "name": "pod_type",
            "type": "String"
          }
        ]
      },
      {
        "name": "Status",
        "fields": [
          {
            "name": "waiting",
            "type": "List",
            "ofType": "String"
          },
          {
            "name": "crashed",
            "type": "List",
            "ofType": "String"
          },
          {
            "name": "host",
            "type": "String"
          },
          {
            "name": "port",
            "type": "Int"
          },
          {
            "name": "state",
            "type": "String"
          },
          {
            "name": "ready",
            "type": "Boolean"
          }
        ]
      },
      {
        "name": "Tests",
        "actions": [
          {
            "name": "testGetDatabases",
            "type": "Boolean"
          }
        ]
      }
    ]
  },
  "dependencies": {
    "http": "http:"
  }
}